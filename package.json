  "scripts": {
    "build-storybook": "build-storybook",
    "format": "npm run format:package && npm run format:prettier && npm run format:eslint && npm run format:scss",
    "format:eslint": "eslint --ext .ts,.tsx,.json src/ --fix",
    "format:package": "sort-package-json",
    "format:prettier": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "json-server": "json-server json.server.js --routes routes.json --port 5000",
    "lint": "npm run lint:ts && npm run lint:code && npm run lint:scss",
    "lint:code": "eslint --ext .ts,.tsx,.json src/",
    "lint:commit": "commitlint -e",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:ts": "tsc --noEmit",
    "pre-commit": "lint-staged",
    "prepare": "husky install",
    "selenium:start": "selenium-standalone start",
    "start": "cross-env NODE_ENV=development TS_NODE_PROJECT=\"tsconfig.webpack.json\" webpack serve",
    "storybook": "start-storybook -p 6006",
    "test": "jest --runInBand",
    "previsual-tests": "tsc --incremental -p tsconfig.visual-tests.json",
    "visual-tests": "playwright test",
    "previsual-tests:generate": "tsc --incremental -p tsconfig.visual-tests.json",
    "visual-tests:generate": "playwright test --update-snapshots",
    "previsual-tests:ui": "tsc --incremental -p tsconfig.visual-tests.json",
    "visual-tests:ui": "playwright test --ui"
  },
